<h1>Welcome to the Student Section</h1>
{{# if activities}}
<table>
    <ul>
        {{#each activities}}
        <li>Activity ID: {{this.activityID}}</li>
        <li>Created by: {{this.teacherID}}</li>
        <li>Video: <a href="{{this.videoLink}}">Link</a></li>
        <li>Question IDs: {{this.questionIDs}}</li>
        {{#each this.questions}}
        <li>Question Text: {{this.questionText}}</li>
<li>Choices: {{this.questionChoices}}</li><br>       
         {{/each}}
        <br>
        {{/each}}
    </ul>
</table>
{{/if}}